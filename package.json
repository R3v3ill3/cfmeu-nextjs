{
  "name": "cfmeu-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:app",
    "dev:app": "NEXT_PUBLIC_USE_WORKER_DASHBOARD=true NEXT_PUBLIC_DASHBOARD_WORKER_URL=http://localhost:3100 next dev --turbo -p 3000",
    "dev:all": "npm run stop:ports && concurrently -n app,scraper,dashboard,bci,scanner -c \"cyan,magenta,green,yellow,blue\" -k \"npm run dev:app\" \"npm run worker:scraper:dev\" \"npm run worker:dashboard:dev\" \"npm run worker:bci:dev\" \"npm run worker:scanner:dev\"",
    "dev:all:mac": "npm run stop:ports && concurrently -n app,scraper,dashboard,bci,scanner -c \"cyan,magenta,green,yellow,blue\" -k \"npm run dev:app\" \"npm run worker:scraper:dev:mac\" \"npm run worker:dashboard:dev\" \"npm run worker:bci:dev\" \"npm run worker:scanner:dev:mac\"",
    "worker:scraper:dev": "npm --prefix railway_workers/cfmeu-scraper-worker run dev",
    "worker:scraper:dev:mac": "npm --prefix railway_workers/cfmeu-scraper-worker run dev:mac",
    "worker:dashboard:dev": "PORT=3100 npm --prefix railway_workers/cfmeu-dashboard-worker run dev",
    "worker:bci:dev": "PORT=3250 npm --prefix railway_workers/bci-import-worker run dev",
    "worker:scanner:dev": "npm --prefix railway_workers/mapping-sheet-scanner-worker run dev",
    "worker:scanner:dev:mac": "npm --prefix railway_workers/mapping-sheet-scanner-worker run dev:mac",
    "stop:ports": "bash -lc 'for p in 3000 3001 3101 3250; do f=$(lsof -ti :$p); if [ -n \"$f\" ]; then echo Killing $p: $f; kill -9 $f || true; fi; done'",
    "stop:all": "pkill -f \"concurrently -n app,scraper,dashboard,bci\" || true; npm run -s stop:ports",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate-trades": "tsx scripts/migrateTrades.ts",
    "refresh-views": "node scripts/daily-refresh.js",
    "probe:incolink": "tsx scripts/incolink_probe.ts 7125150",
    "check-view-staleness": "curl -s ${NEXT_PUBLIC_APP_URL:-http://localhost:3000}/api/admin/refresh-views | jq '.views'",
    "embed-docs": "tsx scripts/embed-docs.ts"
  },
  "engines": {
    "node": ">=18.18"
  },
